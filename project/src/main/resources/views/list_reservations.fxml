    <children>
        <TableView fx:id="reservationsTableView" prefHeight="400.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="idColumn" prefWidth="50.0" text="ID" />
                <TableColumn fx:id="rehlaIdColumn" prefWidth="80.0" text="Rehla ID" />
                <TableColumn fx:id="userNameColumn" prefWidth="120.0" text="User Name" />
                <TableColumn fx:id="emailColumn" prefWidth="150.0" text="Email" />
                <TableColumn fx:id="contactColumn" prefWidth="100.0" text="Contact" />
