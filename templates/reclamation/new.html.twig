       {% block form %}
            <style>
                .card {
                width: 100%;
                max-width: 400px;
                margin: 20px auto;
                border: 1px solid #ccc;
                border-radius: 8px;
                overflow: hidden;
                box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
            }

            .card-header {
                padding: 10px 15px; /* Reduced padding */
                background-color: #f7f7f7;
                border-bottom: 1px solid #ccc;
                text-align: center;
            }

            .card-header h3 {
                margin: 0; /* Remove default margin */
                padding: 0; /* Remove default padding */
                font-size: 1.5rem;
            }

            .card-body {
                padding: 10px 15px; /* Reduced padding */
            }

            .card-body form {
                margin: 0; /* Remove default margin from the form */
                padding: 0; /* Remove default padding from the form */
            }

            .card-body .form-group {
                margin-bottom: 10px; /* Add spacing between form fields */
            }

            .card-footer {
                padding: 10px;
                background-color: #f7f7f7;
                border-top: 1px solid #ccc;
                text-align: right;
            }

            .card-footer button {
                padding: 8px 16px;
                margin-left: 8px;
                border: none;
                border-radius: 4px;
                cursor: pointer;
            }

            .card-footer button[type="submit"] {
                background-color: #007bff;
                color: white;
            }

            .card-footer button[type="reset"] {
                background-color: #6c757d;
                color: white;
            }
            </style>




            <div class="card">
