<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Alert?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.controllers.UpdateParticipationController">
    <TextField fx:id="tfNom" promptText="Nom" />
    <TextField fx:id="tfEmail" promptText="Email" />
    <TextField fx:id="tfTelephone" promptText="Téléphone" />
    <TextField fx:id="tfEventId" promptText="ID de l'événement" visible="false" managed="false"/>
    <Button fx:id="btnUpdate" text="Mettre à jour" onAction="#handleUpdate"/>
</VBox>
